{
  "version": 2,
  "name": "pokeDuel",
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "app.js"
    }
  ],
  "headers": {
    "source": "/(.*)",
    "headers": [
      {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      }
    ]
  }
}